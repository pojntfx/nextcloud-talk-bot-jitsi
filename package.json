{
  "name": "@pojntfx/nextcloud-talk-bot-jitsi",
  "version": "1.0.1",
  "description": "A bot for Nextcloud Talk that creates Jitsi meetings.",
  "main": "main.js",
  "repository": "git@github.com:pojntfx/nextcloud-talk-bot-jitsi.git",
  "author": "Felicitas Pojtinger <felicitas@pojtinger.com>",
  "license": "AGPL-3.0",
  "private": false,
  "devDependencies": {
    "grpc-tools": "^1.8.1"
  },
  "dependencies": {
    "@pojntfx/jitsi-meet-node-client": "^1.0.3",
    "google-protobuf": "^3.11.4",
    "grpc": "^1.24.2"
  },
  "scripts": {
    "build": "mkdir -p ./protos/generated && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./protos/generated --grpc_out=./protos/generated --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I=./protos ./protos/*.proto",
    "start": "node main.js"
  }
}
